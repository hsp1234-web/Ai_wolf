    "get_ipython().system('pip install streamlit google-generativeai yfinance pandas fredapi requests -q')\n",
    "print(\"✅ 套件安裝完成！\")\n",
    "\n",
    "GDRIVE_PROJECT_DIR = \"/content/drive/MyDrive/wolfAI\" # Google Drive 中的專案路徑\n",
    "LOCAL_PROJECT_DIR = \"/content/wolfAI\" # 本地 Colab 儲存的專案路徑\n",
    "PROJECT_DIR_TO_USE = None # 實際使用的專案目錄\n",
    "\n",
    "if IN_COLAB:\n",
    "    print(\"\\n[2/5] 正在嘗試掛載 Google Drive...\")\n",
    "    try:\n",
    "        drive.mount('/content/drive', force_remount=True)\n",
    "        print(\"✅ Google Drive 掛載成功！\")\n",
    "        PROJECT_DIR_TO_USE = GDRIVE_PROJECT_DIR\n",
    "\\n",
    "        # --- 創建 Wolf_Data 目錄結構 (僅當 Drive 掛載成功時) ---\\n",
    "        WOLF_DATA_BASE_GDRIVE = \"/content/drive/MyDrive/Wolf_Data\"\\n",
    "        print(f\"    檢查/創建 Wolf_Data 基礎目錄: {WOLF_DATA_BASE_GDRIVE}\")\\n",
    "        if not os.path.exists(WOLF_DATA_BASE_GDRIVE):\\n",
    "            os.makedirs(WOLF_DATA_BASE_GDRIVE, exist_ok=True)\\n",
    "            print(f\"    ✅ '{WOLF_DATA_BASE_GDRIVE}' 已創建。\")\\n",
    "        else:\\n",
    "            print(f\"    ➡️ '{WOLF_DATA_BASE_GDRIVE}' 已存在。\")\\n",
    "\\n",
    "        sub_dirs_to_create = [\"source_documents/masters\", \"source_documents/shan_jia_lang\", \"weekly_reports\", \"logs\"]\\n",
    "        for sub_dir in sub_dirs_to_create:\\n",
    "            full_sub_dir_path = os.path.join(WOLF_DATA_BASE_GDRIVE, sub_dir)\\n",
    "            print(f\"    檢查/創建子目錄: {full_sub_dir_path}\")\\n",
    "            if not os.path.exists(full_sub_dir_path):\\n",
    "                os.makedirs(full_sub_dir_path, exist_ok=True)\\n",
    "                print(f\"    ✅ '{full_sub_dir_path}' 已創建。\")\\n",
    "            else:\\n",
    "                print(f\"    ➡️ '{full_sub_dir_path}' 已存在。\")\\n",
    "        # --- Wolf_Data 目錄結構創建完畢 ---\\n",
    "\\n",
    "    except Exception as e:\n",
    "        print(f\"⚠️ Google Drive 掛載失敗: {e}\")\n",
    "        print(\"將嘗試使用 Colab 本地儲存空間。如果重新執行，之前在 Drive 中的資料可能不會被使用。\")\n",
    "        PROJECT_DIR_TO_USE = LOCAL_PROJECT_DIR\n",
    "else:\n",
    "    print(\"\\n[2/5] 跳過 Google Drive 掛載 (非 Colab 環境)。\")\n",
    "    PROJECT_DIR_TO_USE = LOCAL_PROJECT_DIR # 本地開發時使用此路徑\n",
    "\n",
    "# Wolf_Data 路徑設定 (應用程式日誌將寫到 Wolf_Data/logs)\\n",
    "# 注意: app.py 中也會定義 BASE_DATA_DIR，這裡主要是為了 notebook 自身的日誌路徑提示和目錄檢查\\n",
    "WOLF_DATA_GDRIVE_BASE_INFO = \"/content/drive/MyDrive/Wolf_Data\" # 標準化, for info\\n",
    "WOLF_DATA_LOCAL_BASE_INFO = \"/content/Wolf_Data\" # 標準化 (如果 Colab 本地運行), for info\\n",
    "APP_EXPECTED_LOG_DIR = None # 用於顯示 app.py 將使用的日誌路徑\\n",
    "\n",
    "if PROJECT_DIR_TO_USE == GDRIVE_PROJECT_DIR: # 如果專案在 Drive，則 Wolf_Data 也在 Drive\\n",
    "    APP_EXPECTED_LOG_DIR = os.path.join(WOLF_DATA_GDRIVE_BASE_INFO, \"logs\")\\n",
    "    # The Wolf_Data base and subdirs are already created above if Drive mount was successful.\\n",
    "    # Perform a sanity check for the base if needed, though it should exist.\\n",
    "    if not os.path.exists(WOLF_DATA_GDRIVE_BASE_INFO):\\n",
    "        print(f\"    ⚠️ Sanity Check: Wolf_Data base {WOLF_DATA_GDRIVE_BASE_INFO} not found, creating.\")\\n",
    "        os.makedirs(WOLF_DATA_GDRIVE_BASE_INFO, exist_ok=True)\\n",
    "else: # 否則 Wolf_Data 在本地 (Colab 的 /content/Wolf_Data or local non-Colab user dir based on app.py)\\n",
    "    # For notebook info, assume app.py will use a local Wolf_Data structure\\n",
    "    # The actual local path depends on app.py's logic (os.path.expanduser(\"~\")), \\n",
    "    # so this is just an illustrative path for Colab's local context.\\n",
    "    APP_EXPECTED_LOG_DIR = os.path.join(WOLF_DATA_LOCAL_BASE_INFO, \"logs\")\\n",
    "    if not os.path.exists(WOLF_DATA_LOCAL_BASE_INFO): # 確保本地 Wolf_Data 基礎目錄存在 (for Colab local)\\n",
    "        os.makedirs(WOLF_DATA_LOCAL_BASE_INFO, exist_ok=True)\\n",
    "        print(f\"    ✅ 已確認/建立模擬的本地 Wolf_Data 基礎目錄: {WOLF_DATA_LOCAL_BASE_INFO}\")\\n",
    "\n",
    "print(f\"\\n[3/5] 設定專案路徑: {PROJECT_DIR_TO_USE}\")\n",
    "print(f\"    應用程式資料/日誌目錄預期在: {APP_EXPECTED_LOG_DIR} (實際路徑由 app.py 設定)\")\\n",
    "\n",
    "# 強制清理舊的專案目錄\n",
    "if os.path.exists(PROJECT_DIR_TO_USE):\n",
    "    print(f\"    偵測到已存在的專案目錄 '{PROJECT_DIR_TO_USE}'。執行強制清理...\")\n",
    "    try:\n",
    "        # 使用 shutil.rmtree 比 !rm -rf 更安全，且是 Python 原生\n",
    "        import shutil\n",
    "        shutil.rmtree(PROJECT_DIR_TO_USE)\n",
    "        print(f\"    ✅ '{PROJECT_DIR_TO_USE}' 已成功刪除。\")\n",
    "    except Exception as e_rm:\n",
    "        print(f\"    ❌ 刪除 '{PROJECT_DIR_TO_USE}' 時發生錯誤: {e_rm}\")\n",
    "        print(\"       請手動檢查並刪除該資料夾，然後重新執行此儲存格。\")\n",
    "        raise # 阻止後續執行\n",
    "else:\n",
    "    print(f\"    '{PROJECT_DIR_TO_USE}' 不存在，無需清理。\")\n",
    "\n",
    "# 確保專案的父目錄存在 (例如 /content/drive/MyDrive)\n",
    "# git clone 會創建 PROJECT_DIR_TO_USE 本身\n",
    "project_parent_dir = os.path.dirname(PROJECT_DIR_TO_USE)\n",
    "if not os.path.exists(project_parent_dir) and PROJECT_DIR_TO_USE != LOCAL_PROJECT_DIR: # LOCAL_PROJECT_DIR 的父目錄 /content 通常存在\n",
    "     os.makedirs(project_parent_dir, exist_ok=True) # 創建父目錄，例如 /content/drive/MyDrive\n",
    "\n",
    "# MyWolfData/logs 目錄由 app.py 內部邏輯創建，這裡僅作提示\n",
    "# 但如果希望 Colab 腳本也確保 MyWolfData 基礎目錄存在，可以加上：\n",
    "# my_wolf_data_base = os.path.dirname(MY_WOLF_DATA_LOG_DIR) # Old MyWolfData logic removed\\n",
    "# if not os.path.exists(my_wolf_data_base): # Old MyWolfData logic removed\\n",
    "#     os.makedirs(my_wolf_data_base, exist_ok=True) # Old MyWolfData logic removed\\n",
    "#     print(f\"    ✅ 已確認/建立 MyWolfData 基礎目錄: {my_wolf_data_base}\") # Old MyWolfData logic removed\\n",
    "\n",
    "\n",
    "STREAMLIT_APP_PATH = os.path.join(PROJECT_DIR_TO_USE, \"app.py\")\n",
    "REQUIREMENTS_PATH = os.path.join(PROJECT_DIR_TO_USE, \"requirements.txt\")\n",
    "PORT = 8501\n",
    "\n",
    "print(f\"✅ 已確認/建立專案目錄。\") # PROJECT_DIR_TO_USE 會在 git clone 時建立\n",
    "\n",
    "print(f\"\\n[4/5] 正在從 GitHub 獲取最新程式碼...\")\n",
    "GIT_REPO_URL = \"https://github.com/hsp1234-web/Ai_wolf.git\" # 使用者提供的原始倉庫\n",
    "# 注意：之前使用者 feedback 中有一個 hsp1234-web 的 fork，這裡用計畫書中的原始倉庫\n",
    "\n",
    "# 使用 subprocess.run 執行 git clone，更易於錯誤處理\n",
    "git_clone_command = [\"git\", \"clone\", \"--depth\", \"1\", GIT_REPO_URL, PROJECT_DIR_TO_USE]\n",
    "result = subprocess.run(git_clone_command, capture_output=True, text=True)\n",
    "if result.returncode == 0:\n",
    "    print(\"    ✅ 專案克隆完成！\")\n",
    "else:\n",
    "    print(f\"    ❌ 專案克隆失敗:\")\n",
    "    print(result.stdout)\n",
    "    print(result.stderr)\n",
    "    raise SystemExit(\"Git clone 失敗，終止執行。\")\n",
    "\n",
    "# 使用 requirements.txt 安裝依賴\n",
    "if os.path.isfile(REQUIREMENTS_PATH):\n",
    "    print(\"    正在從 requirements.txt 安裝依賴...\")\n",
    "    pip_install_command = [\"pip\", \"install\", \"-q\", \"-r\", REQUIREMENTS_PATH]\n",
    "    result_pip = subprocess.run(pip_install_command, capture_output=True, text=True)\n",
    "    if result_pip.returncode == 0:\n",
    "        print(\"    ✅ 依賴安裝完成！\")\n",
    "    else:\n",
    "        print(f\"    ❌ 依賴安裝失敗:\")\n",
    "        print(result_pip.stdout) # pip -q 通常不輸出到 stdout\n",
    "        print(result_pip.stderr)\n",
    "        # 即使 pip install 失敗，也可能部分成功，所以不一定 raise SystemExit\n",
    "else:\n",
    "    print(f\"    ⚠️ 警告: 未找到 requirements.txt 於 {REQUIREMENTS_PATH}。跳過依賴安裝步驟。\")\n",
    "\n",
    "\n",
    "print(f\"\\n[5/5] 檢查主要應用程式檔案...\")\n",
    "if not os.path.isfile(STREAMLIT_APP_PATH):\n",
    "    raise SystemExit(f\"❌ 錯誤：未在 {PROJECT_DIR_TO_USE} 中找到 app.py！\")\n",
    "print(f\"✅ 成功找到主要應用程式檔案: {STREAMLIT_APP_PATH}\")\n",
    "print(\"\\n--- 環境設置與專案部署階段完成 ---\\n\")\n",
    "time.sleep(2)\n",
    "if IN_COLAB: clear_output(wait=True)\n",
    "\n",
    "# --- 階段二：啟動 Streamlit 並直接監控其輸出 ---\n",
    "print(\"--- 階段二：啟動 Streamlit 並直接監控其輸出 ---\")\n",
    "streamlit_process = None\n",
    "monitor_thread = None\n",
    "tunnel_url = None # 用於 cloudflared\n",
    "\n",
    "try:\n",
    "    # 啟動 Streamlit 應用程式\n",
    "    # 使用 sys.executable 確保是用當前 Colab 環境的 Python 解釋器\n",
    "    import sys\n",
    "    cmd_streamlit = [sys.executable, \"-m\", \"streamlit\", \"run\", STREAMLIT_APP_PATH, \"--server.port\", str(PORT), \"--server.headless\", \"true\", \"--server.enableCORS\", \"false\", \"--server.enableXsrfProtection\", \"false\"]\n",
    "\n",
    "    # 【重大改進】將 stderr 重定向到 stdout，以便捕獲所有輸出，包括錯誤\n",
    "    streamlit_process = subprocess.Popen(cmd_streamlit, stdout=subprocess.PIPE, stderr=subprocess.STDOUT, text=True, encoding='utf-8', cwd=PROJECT_DIR_TO_USE)\n",
    "    print(\"✅ Streamlit 啟動指令已送出。\")\n",
    "    print(\"⏳ 等待伺服器初始化並嘗試獲取連結...\")\n",
    "\n",
    "    def stream_watcher(identifier, stream):\n",
    "        for line in iter(stream.readline, ''):\n",
    "            if not line:\n",
    "                break\n",
    "            print(f\"[{identifier}] {line}\", end='', flush=True)\n",
    "        stream.close()\n",
    "\n",
    "    monitor_thread = threading.Thread(target=stream_watcher, args=('Streamlit', streamlit_process.stdout))\n",
    "    monitor_thread.daemon = True # 設置為守護線程，以便主線程結束時它也結束\n",
    "    monitor_thread.start()\n",
    "\n",
    "    # 檢查是否需要使用 cloudflared (例如在 Colab 中，eval_js 可能失敗或不適用於所有情況)\n",
    "    # Colab 的 google.colab.kernel.proxyPort 通常更直接\n",
    "    if IN_COLAB: # Check if running in Colab\\n",
    "        print(\"\\n✅ 伺服器已在背景啟動。\")\\n",
    "        print(\"   請等待約 15-30 秒讓服務完成初始化...\")\\n",
    "        try:\\n",
    "            input(\"   初始化完成後，請按 Enter 鍵嘗試獲取應用程式網址...\")\\n",
    "        except KeyboardInterrupt:\\n",
    "            print(\"\\n⌨️ 操作被使用者中斷。正在嘗試停止服務...\")\\n",
    "            raise # Re-raise to trigger the finally block for cleanup\\n",
    "        except Exception as e:\\n",
    "            print(f\"\\n⚠️ 讀取用戶輸入時發生錯誤: {e}。將繼續嘗試獲取URL。\")\\n",
    "\\n",
    "        # Stage 2: URL Fetching with Retry\\n",
    "        print(\"\\n🔗 開始嘗試獲取應用程式網址...\")\\n",
    "        MAX_RETRIES = 5\\n",
    "        RETRY_DELAY_SECONDS = 5 # Retry delay in seconds\\n",
    "        \\n",
    "        # Ensure tunnel_url is initialized to None if it's not already defined earlier\\n",
    "        # In the provided script, tunnel_url is initialized to None before the try block, which is good.\\n",
    "        \\n",
    "        for attempt in range(MAX_RETRIES):\\n",
    "            print(f\"   第 {attempt + 1}/{MAX_RETRIES} 次嘗試...\")\\n",
    "            try:\\n",
    "                # Ensure eval_js is available (it's imported at the top of the script)\\n",
    "                proxy_url_eval = eval_js(f'google.colab.kernel.proxyPort({PORT})', timeout_sec=10) # timeout_sec is optional but good\\n",
    "                \\n",
    "                if proxy_url_eval:\\n",
    "                    tunnel_url = proxy_url_eval # Assign to the main tunnel_url variable\\n",
    "                    print(f\"   🎉 成功獲取 Colab 代理 URL: {tunnel_url}\")\\n",
    "                    break # Exit loop on success\\n",
    "                else:\\n",
    "                    print(f\"   ⚠️ 第 {attempt + 1} 次嘗試：eval_js 未返回有效 URL。\")\\n",
    "            except NameError: \\n",
    "                print(\"   ❌ 錯誤：eval_js 未定義。無法獲取 Colab 代理 URL。\")\\n",
    "                break \\n",
    "            except Exception as e:\\n",
    "                # Check if the error message is about 'google.colab.kernel.proxyPort' not being available\\n",
    "                if 'proxyPort is not defined' in str(e) or 'google.colab.kernel.proxyPort is not a function' in str(e):\\n",
    "                    print(f\"   ⚠️ 第 {attempt + 1} 次嘗試：Colab Kernel ProxyPort 功能目前不可用。({e})\")\\n",
    "                else:\\n",
    "                    print(f\"   ⚠️ 第 {attempt + 1} 次嘗試獲取 URL 時發生錯誤: {e}\")\\n",
    "            \\n",
    "            if attempt < MAX_RETRIES - 1:\\n",
    "                print(f\"   將在 {RETRY_DELAY_SECONDS} 秒後重試...\")\\n",
    "                time.sleep(RETRY_DELAY_SECONDS) # Make sure time module is imported\\n",
    "            else:\\n",
    "                print(f\"   ❌ 達到最大重試次數 ({MAX_RETRIES})，仍未能透過 eval_js 獲取網址。\")\\n",
    "        \\n",
    "        if not tunnel_url:\\n",
